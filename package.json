{
    "name": "orbis-example",
    "description": "Orbis example project.",
    "version": "0.0.0",
    "license": "MIT",
    "author": "Daniel Huisman <daniel@huisman.me> (https://danielhuisman.io)",
    "repository": "github:DanielHuisman/orbis-example",
    "keywords": [
        "orbis",
        "example"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "LICENSE.md",
        "README.md",
        "package.json"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "yarn run clean && yarn run build:ts",
        "build:ts": "tsc",
        "prepublishOnly": "yarn run build",
        "dev": "dotenv -- nodemon -e ts -x ts-node src/index.ts",
        "start": "dotenv -- node dist/index.js",
        "typeorm": "ts-node ./node_modules/.bin/typeorm"
    },
    "dependencies": {
        "@nexus/schema": "^0.15.0",
        "@orbis-framework/auth": "^0.1.1",
        "@orbis-framework/core": "^0.1.1",
        "graphql": "^15.3.0",
        "pg": "^8.3.0",
        "typeorm": "^0.2.25"
    },
    "devDependencies": {
        "dotenv-cli": "^3.2.0",
        "nodemon": "^2.0.4",
        "rimraf": "^3.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    }
}
